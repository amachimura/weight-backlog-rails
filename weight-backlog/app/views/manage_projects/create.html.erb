<h1>ManageProjects#create</h1>
<p>Find me in app/views/manage_projects/create.html.erb</p>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(:manage_project, url: 'create') do |f| %>
        <%= f.label :project_name %>
        <%= f.text_field :project_name, class: 'form-control' %>

        <%= f.label :target_date %>
        <%= f.date_select :target_date, class: 'form-control' %>

        <%= f.label :target_weight %>
        <%= f.number_field :target_weight %>

        <%= f.submit "Register", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>
